  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "allowJs": true,
    "checkJs": false,
    "jsx": "react-native",
    "lib": [
      "dom",
      "esnext"
    ],
    "target": "es6",
    "moduleResolution": "node",
    "noEmit": true,
    "skipLibCheck": true,
    "resolveJsonModule": true
